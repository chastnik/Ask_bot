{
  "app": "app.main:app",
  "host": "0.0.0.0",
  "port": 8000,
  "reload": true,
  "reload_dirs": [
    "app"
  ],
  "reload_includes": [
    "*.py"
  ],
  "reload_excludes": [
    "venv/*",
    ".venv/*",
    "venv/**/*",
    ".venv/**/*",
    "*.egg-info/*",
    "__pycache__/*",
    "*.pyc",
    "*.pyo",
    "*.pyd",
    ".git/*",
    "*.log",
    "*.db",
    "charts/*",
    "logs/*",
    "tmp/*",
    "temp/*",
    ".pytest_cache/*",
    "build/*",
    "dist/*"
  ]
} 